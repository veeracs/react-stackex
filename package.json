{
  "name": "react-stackex",
  "version": "1.0.0",
  "description": "React StackEx",
  "main": "client.js",
  "scripts": {
    "start": "NODE_ENV=production DB_ENV=production npm run build && node lib/server.js",
    "build": "npm run clean && NODE_ENV=production webpack",
    "watch": "npm run clean && webpack --watch",
    "server": "npm run build && forever stopall && NODE_ENV=production DB_ENV=production SSL_ENV=production forever start src/server.js",
    "clean": "rm -rf public/build && rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veeracs/react-stackex.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Webpack"
  ],
  "author": "Chandra Sekhar Veera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/veeracs/react-stackex/issues"
  },
  "homepage": "https://github.com/veeracs/react-stackex#readme",
  "dependencies": {
    "compression": "^1.7.0",
    "express": "^4.15.3",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0"
  }
}
